{
    "name": "@timmehx/tree",
    "version": "0.0.2",
    "private": true,
    "description": "",
    "sideEffects": false,
    "main": "./dist/tsc/main.js",
    "types": "./dist/tsc/main.d.ts",
    "author": "Tim Speelman",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "scripts": {
        "start": "webpack serve --config ./webpack.dev.js",
        "lint": "eslint --ext ts,tsx src",
        "lint:fix": "eslint --fix --ext ts,tsx src",
        "test": "jest --maxWorkers=1",
        "test:watch": "jest --maxWorkers=1 --watchAll",
        "tsc": "tsc --noEmit --project ./tsconfig.json",
        "tsc:watch": "tsc --noEmit --watch --project ./tsconfig.json",
        "clean": "rm -rf dist build package",
        "ts-node": "ts-node",
        "docs": "typedoc --entryPoints src/main.ts",
        "build": "tsc -p tsconfig.json",
        "build-all": "npm run clean && npm run build ",
        "esbuild-browser": "esbuild src/browser.ts --bundle --minify --sourcemap=external --outfile=dist/esbuild/browser.js",
        "esbuild-browser:dev": "esbuild src/browser.ts --bundle --outfile=dist/esbuild/browser.js",
        "esbuild-browser:watch": "esbuild src/browser.ts --bundle --watch --outfile=dist/esbuild/browser.js",
        "patch-release": "npm run build-all && npm version patch && npm publish && git push --follow-tags",
        "minor-release": "npm run build-all && npm version minor && npm publish && git push --follow-tags",
        "major-release": "npm run build-all && npm version major && npm publish && git push --follow-tags"
    },
    "devDependencies": {
        "@types/jest": "^27.5.2",
        "jest": "^28.1.3",
        "jest-each": "^29.7.0",
        "ts-jest": "^28.0.8",
        "typescript": "^5.8.3"
    },
    "dependencies": {}
}
